{
  "info": {
    "name": "Nutri-B2B Backend",
    "_postman_id": "b2b-collection-1",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "health",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{BASE}}/health",
          "host": [
            "{{BASE}}"
          ],
          "path": [
            "health"
          ]
        }
      }
    },
    {
      "name": "healthz",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{BASE}}/healthz",
          "host": [
            "{{BASE}}"
          ],
          "path": [
            "healthz"
          ]
        }
      }
    },
    {
      "name": "products",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-Appwrite-JWT",
            "value": "{{JWT}}"
          }
        ],
        "url": {
          "raw": "{{BASE}}/products",
          "host": [
            "{{BASE}}"
          ],
          "path": [
            "products"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "BASE",
      "value": "http://localhost:5000"
    },
    {
      "key": "JWT",
      "value": ""
    }
  ]
}